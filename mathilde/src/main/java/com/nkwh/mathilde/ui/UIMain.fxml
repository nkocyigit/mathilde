<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.nkwh.mathilde.ui.UIMainController">
   <center>
      <BorderPane id="paneMain" prefHeight="600.0" prefWidth="750.0" BorderPane.alignment="CENTER" />
   </center>
   <top>
      <MenuBar BorderPane.alignment="CENTER">
        <menus>
          <Menu fx:id="mnFile" mnemonicParsing="false" text="File">
            <items>
              <MenuItem fx:id="mnItemClose" mnemonicParsing="false" text="Close" />
            </items>
          </Menu>
          <Menu fx:id="mnEdit" mnemonicParsing="false" text="Edit" />
          <Menu fx:id="mnHelp" mnemonicParsing="false" text="Help">
            <items>
                  <MenuItem fx:id="mnItemPreferences" mnemonicParsing="false" onAction="#menuItemPreferencesClicked" text="Preferences" />
              <MenuItem fx:id="mnItemAbout" mnemonicParsing="false" text="About" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
   </top>
</BorderPane>
